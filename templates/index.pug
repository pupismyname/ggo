extends partials/layout.pug

block content
  #content.container.space

    #infobar.container.space
      .box.type5
        .container.maininfo
          h2.box.type5c.first September 27-30, 2017
          h3.box.type5c.first Warner Robins, GA
          p.box.type2.first
            a#appbutton(href='files/GGOApp2017-fINAL.pdf') <span>2017</span> Application
          .box.type3c
            ul
              li: a(href='files/Rule Book 2017 (FINAL OFFICIAL)1.pdf') 2017 Event List and Rules
              li: a(href='files/July 2017 ggo newletter.pdf') July 2017 Newsletter
              li: a(href='files/2017 Golden Games Map.pdf') Event Map
              li: a(href='files/results report web 10-30-16.pdf') 2016 Results
        .container.lowerinfo
          .box.type5c.first
            p Returning competitors can <a href='http://ggoregistration.org/forms/ggoregistrationform1/ggoregistrationform1.php'>register online</a>.
            p The 2017 Georgia Golden Olympics is scheduled for September 27-30, 2017 in Warner Robins, GA. Join us for fun, fellowship, and competition.
            p <a href='http://www.nsga.com/'><img class='inline' src='images/nsga.gif' alt='NSGA Logo' title='National Senior Games Association' /> <img class='inline' src='images/NSG17_FINAL.png' alt='NSGA Logo' alt='National Senior Games Logo' title='National Senior Games' /></a> The 2017 <a href='http://www.nsga.com/' target='_blank'>National Senior Games</a> is scheduled for June 2017 in Birmingham, AL.
      .box.type7
        .container
          #picfade.box.type7.first
            img#defaultpic(src='images/stack1.jpg', alt='', title='')
          .social
            .box.type7c.first
              p.detail Follow Georgia Golden Olympics on <a href='http://twitter.com/gagoldolympics' target='_new'><img src='images/icon-twitter.png' alt='Twitter' title='Twitter' /></a> and <a href='http://www.facebook.com/pages/Georgia-Golden-Olympics/200148176686061' target='_new'><img src='images/icon-facebook.png' alt='Facebook' title='Facebook' /></a>

    #types.container.gap.bg444

      .box.type4.participate
        h4.box.type4c.first
          a(href='participate.html') Participate
        .box.type4c.first
