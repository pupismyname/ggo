doctype html
html
  head
    meta(http-equiv="Content-Type", content="text/html; charset=iso-8859-1")
    title Georgia Golden Olympics
      if filename !== 'index'
        |  - #{name}
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,700', rel='stylesheet', type='text/css')
    link(rel="stylesheet", type="text/css", href="styles/ggo.css")
  -
    var idAtt = 'ggo-' + type
    var classAtt = 'nogrid ggo' + filename
  body(id=idAtt, class=classAtt)
    #wrapper
      #header.container
        h1.box.type3c
          a(href="index.html") Georgia Golden Olympics
        ul#mainmenu.box.type8.pad1
          li.box.type2.participate.first
            a(href="participate.html") Participate
          li.box.type2.volunteer
            a(href="volunteer.html") Volunteer
          li.box.type2.sponsor
            a(href="sponsor.html") Sponsor
          li.box.type2.about
            a(href="about.html") About GGO

      block content

      #footer.container
        .box.type4c
          p &copy; #{copyright} <a href="index.html">Georgia Golden Olympics</a>
        .box.type5c
          p Contact GGO Coordinator <a href="mailto:vcpilgrim@gmail.com">Vicki Pilgrim</a> at 770-867-3603
        .box.type3c.last
          p Design by <a href="http://www.pupismyname.com/">John Brooks</a>

    script(src="jquery/jquery-1.2.3.min.js")
    script(src="jquery/jquery.easing.1.3.js")
    script(src="ggo.js")

    include ga
