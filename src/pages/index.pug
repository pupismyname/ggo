            .box.type6c.first
              p.detail $25-$99
              ul
                +sponsor("Betty Gray", "Mableton")
                +sponsor("John Bunnell", "Dahlonega")
                +sponsor("Alan & Judy Johnson", "Lawrenceville")
                +sponsor("LL Howard & Christi Collier", "Colorado Springs, CO, In memory of Jan Collier")

mixin sponsor (name, location, src, link)
  li
    if link
      a(href!=link, target="_new")
        +sponsor-internal(name, location, src)
    else
      +sponsor-internal(name, location, src)

mixin sponsor-internal (name, location, src)
  if src
    img(src!=src, alt!=name, title!=name)
  =name
  if location
    |
    |
    span=location
