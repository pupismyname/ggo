extends /layout.pug

block content
  #content.container.space
    #infobar.container.space
      .wrapper
        .box.type5
          .container.maininfo
            h2.box.type5c.first September 25-28, 2019
            h3.box.type5c.first Warner Robins, GA
            p.box.type2.first
              a#appbutton(href="/files/2019 Georgia Golden Olympics Results Report 11-13-19.pdf") #[span 2019] Results
              //- a#appbutton(href="/files/2019 GGO Registration Form.pdf") #[span 2019] Registration
            .box.type3c
              ul
                li: a(href="/files/Rule Book 2019 final.pdf") 2019 Event List and Rules
                li: a(href="/files/Feb 2019 Newsletter.pdf") February 2019 Newsletter
                li: a(href="/files/2017 Golden Games Map.pdf") Event Map
                li: a(href="/files/2018 OFFICIAL RESULTS WEBSITE 10-25-18.pdf") 2018 Results
          .container.lowerinfo
            .box.type5c.first
              p The 2019 Georgia Golden Olympics were held September 25-28, 2019 in Warner Robins, GA.
              p The Pickleball Event was held October 1-3, 2019 in Griffin, GA at the Spalding County Pickleball Complex at Wyomia Tyus Olympic Park.
              p 2019 was not a qualifying year for the National Senior Games. 2020 would have been a qualifying games for the 2021 National Games in Fort Lauderdale, FL, but the 2020 games have been cancelled.
              p Thank you for joining us for fun, fellowship, and competition.
              //- p Returning competitors can #[a(href="http://ggoregistration.org/forms/ggoregistrationform1/ggoregistrationform1.php") register online].
        .box.type7
          .container
            #picfade.box.type7.first
              img.pic-active(src="/images/stack1.jpg" alt="" title="")
              img(src="/images/stack2.jpg" alt="" title="")
              img(src="/images/stack3.jpg" alt="" title="")
              img(src="/images/stack4.jpg" alt="" title="")
              img(src="/images/stack5.jpg" alt="" title="")
            .social
              .box.type7c.first
                p #[a(href="http://www.nsga.com/") #[img.inline(src="/images/nsga.gif" alt="NSGA Logo" title="National Senior Games Association")] #[img.inline(src="/images/nsg19.png" alt="2017 NSG Logo" title="2017 National Senior Games")]]
                p The 2021 #[a(href="http://www.nsga.com/" target="_blank") National Senior Games] are scheduled for November 5-18, 2021 in Fort Lauderdale, FL.
                p.detail Follow Georgia Golden Olympics on #[a(href="http://twitter.com/gagoldolympics" target="_new") #[img(src="/images/icon-twitter.png" alt="Twitter" title="Twitter")]] and #[a(href="http://www.facebook.com/pages/Georgia-Golden-Olympics/200148176686061" target="_new") #[img(src="/images/icon-facebook.png" alt="Facebook" title="Facebook")]]

    #types.container.gap.bg444
      .wrapper
        .box.type4.participate
          h4.box.type4c.first
            a(href="/participate/") Participate
          .box.type4c.first
            p Are you ready to compete? We have events for every fitness level.
            ul
              li: a(href="/files/2019 Georgia Golden Olympics Results Report 11-13-19.pdf") 2019 Results
              li: a(href="/files/Rule Book 2019 final.pdf") 2019 Event List and Rules
              li: a(href="/files/Feb 2019 Newsletter.pdf") February 2019 Newsletter
              li: a(href="/files/2017 Golden Games Map.pdf") Event Map
              li: a(href="/files/Team Roster Word.pdf") Team Roster
              li Cycling Routes: #[a(href="/files/5K and 10K Cycling Route10.pdf") 5K & 10K], #[a(href="/files/20K Cycling Route-10.pdf") 20K], #[a(href="/files/40K Cycling10.pdf") 40K]

        .box.type4.volunteer
          h4.box.type4c.first
            a(href="/volunteer/") Volunteer
          .box.type4c.first
            p Want to help out? Here's all the information you'll need.
            ul
              li: a(href="/files/Volunteer Packet 2019 Final-1.pdf") Volunteer Signup Form
              li: a(href="/files/Rule Book 2019 final.pdf") 2019 Event List and Rules
              li: a(href="/files/2017 Golden Games Map.pdf") Event Map

        .box.type4.sponsor
          h4.box.type4c.first
            a(href="/sponsor/") Sponsor
          .box.type4c.first
            p These events could not happen without the help of our generous sponsors.
            ul
              li: a(href="http://www.ggoregistration.org/forms/ggodonation/ggodonation.php") Make a donation online!
              li: a(href="/files/ggofundbro.pdf") Sponsorship Brochure
              li: a(href="/sponsor/") Become a Sponsor!

      .box.type12c
        .container
          include /reader

    #sponsors
      .wrapper
        .container.pagetitle.space
          h2.box.type12c 2019 Sponsors
          p Want to be a sponsor? #[a(href="http://www.ggoregistration.org/forms/ggodonation/ggodonation.php") Make a donation online!]

        .container.bg12.space

          .box.type12.corporate
            h4.box.type12c.first Corporate Sponsors
            .box.type12c.first
              p.detail $10,000 or more
              ul
                +sponsor("Georgia Department of Public Health Breast and Cervical Cancer Program", "", "images/sponsors/DPH_Logo_Small_resized.jpg")

        .container.bg66.space

          .box.type6.platinum
            h4.box.type6c.first Platinum Sponsors
            .box.type6c.first
              p.detail $5000-$9999
              ul
                +sponsor("Warner Robins Recreation Department", "Host Agency")

