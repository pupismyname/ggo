        .container.bg66.space

          .box.type6.torch
            h4.box.type6c.first Torch Club
            .box.type6c.first
              p.detail $100-$499
              ul
                +sponsor("Jewel Crews", "Waycross")
                +sponsor("Lester Culver", "Ludowici")
                +sponsor("Harry Lowe", "Waleska")
                +sponsor("James & Miriam Novak", "Bainbridge")
                +sponsor("Shawn Smith", "Dacula")
                +sponsor("James & Jocille Thwing", "Warner Robins")
                +sponsor("Sue Hesse", "Stone Mountain, In memory of Robert Lloyd")
                +sponsor("Robert & Faye Buckham", "Woodstock")
                +sponsor("Rebecca & James Hamilton", "Byron")
                +sponsor("Lois Maat Roberts", "Statesboro")
                +sponsor("Bonnie Romines", "Warner Robins")
                +sponsor("Kenneth Rosskopf", "Decatur")

          .box.type6.friend
