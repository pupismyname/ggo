        .container.bg66.space

          .box.type6.torch
            h4.box.type6c.first Torch Club
            .box.type6c.first
              p.detail $100-$499
              ul
                +sponsor("Jewel Crews", "Waycross")
                +sponsor("Linda Lowery", "Smyrna")
                +sponsor("Gladys Mixon", "Rome")
                +sponsor("James & Miriam Novak", "Bainbridge")
                +sponsor("Allison & Wes Sawyers", "Warner Robins")
                +sponsor("Shawn Smith", "Dacula")
                +sponsor("James & Jocille Thwing", "Warner Robins")
                +sponsor("Robert & Faye Buckham", "Woodstock")
                +sponsor("Lois Maat Roberts", "Statesboro")
                +sponsor("George & Charlotte Zubowicz", "Warm Springs")

          .box.type6.friend
            h4.box.type6c.first Friends
            .box.type6c.first
              p.detail $25-$99
              ul
                +sponsor("Jan & Nicolas Alexuk", "Hinesville")
                +sponsor("John Bunnell", "Dahlonega")
                +sponsor("Robert Jackson", "Lawrenceville")
                +sponsor("Linda Lowery", "Smyrna")
                +sponsor("Martin Marshall", "Lady Lake, FL")
                +sponsor("Mr. & Mrs. Terry Moyer", "Warner Robins")
                +sponsor("Brian Stockmaster", "Charleston, SC")
                +sponsor("Gordon Darrah", "Columbus")

mixin sponsor (name, location, src, link)
  li
    if link
      a(href!=link, target="_new")
        +sponsor-internal(name, location, src)
    else
      +sponsor-internal(name, location, src)

mixin sponsor-internal (name, location, src)
  if src
    img(src!=src, alt!=name, title!=name)
  =name
  if location
    |
    |
    span=location
