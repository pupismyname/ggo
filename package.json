{
  "name": "ggo",
  "version": "1.0.0",
  "description": "Georgia Golden Olympics",
  "main": "index.js",
  "watch": {
    "build:css": {
      "patterns": [
        "src/less"
      ],
      "extensions": "less",
      "quiet": false,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "build": "npm run clean; npm run build:css; npx @11ty/eleventy",
    "build:css": "node -e 'require(\"./src/lib/build-styles.js\")();'",
    "clean": "rm -r ./_site",
    "dev": "npm run clean; npm run build:css; concurrently --kill-others-on-fail \"npm run watch\" \"npx @11ty/eleventy --serve --incremental\"",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pupismyname/ggo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pupismyname/ggo/issues"
  },
  "homepage": "https://github.com/pupismyname/ggo#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "concurrently": "^8.2.1",
    "cssnano": "^6.0.1",
    "less": "^4.1.1",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.28",
    "postcss-import": "^15.1.0"
  }
}
